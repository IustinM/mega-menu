doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=`, initial-scale=1.0")
        meta(http-equiv="X-UA-Compatible", content="ie=edge")
        title Solving mega menu challenge
    body
        ul
            each menu in data.menu
                li
                    a(href=`/${menu.id}`)=menu.label